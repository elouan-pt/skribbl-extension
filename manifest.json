{
  "manifest_version": 2,
  "name": "skribbl-extension",
  "version": "0.1",
  "browser_action": {
    "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
  },
  "content_scripts": [
    {
      "matches": [
        "https://skribbl.io/*"
      ],
      "js": [ "content.js" ]
    }
  ],
  "permissions": [
    "https://www.enchantedlearning.com/*"
  ],
  "background": {
    "scripts": [ "background.js" ]
  }
}
